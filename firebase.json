{
  "hosting": {
    "public": "dist/spa",
    "ignore": ["firebase.json", ".firebaserc", "**/.*", "**/node_modules/**"],
    "rewrites": [
      { "source": "/api/**", "function": "api" },
      { "source": "**", "destination": "/index.html" }
    ]
  },
  "functions": {
    "source": "functions"
  }
}
